@model test_Data.Models.QRCodes
@{
    ViewData["Title"] = "List";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>QR Codes List</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }

        th, td {
            padding: 12px;
            border: 1px solid #ddd;
            text-align: left;
        }

        th {
            background-color: #f2f2f2;
        }

        tr:nth-child(even) {
            background-color: #f9f9f9;
        }
    </style>
</head>
<body>
    <h2>List of Scanned QR Codes</h2>

    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>QR Code Data</th>
                <th>Scanned At</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var qrCode in ViewBag.users)
            {
                <tr>
                    <td>@qrCode.Id</td>
                    <td>@qrCode.Data</td>
                    <td>@qrCode.ScannedAt.ToString("yyyy-MM-dd HH:mm:ss")</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
