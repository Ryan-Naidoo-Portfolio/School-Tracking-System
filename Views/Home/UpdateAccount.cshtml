@model test_Data.Models.AccountModel
@{
    ViewData["Title"] = "UpdateUser";
    AccountModel user = TempData["userTemp"] as AccountModel;
}
<h1>Update Account</h1>
<div class="container">
    <form action="/home/UpdateAccountFinal" method="post">
        <input type="hidden" asp-for="Id" value="@user.Id"/>
        <div class="form-group">
            <label>Username</label>
            <input type="text" asp-for="Username" value="@user.Username" />
            
        </div>
        <div class="form-group">
            <label>Password</label>
            <input type="text" asp-for="Password" value="@user.Password" />
        </div>
        <div class="form-group">
            <label>Position</label>
            <input type="text" asp-for="Position" value="@user.Position" />
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>